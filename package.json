{
  "name": "vue-mushroom-admin-docs",
  "version": "1.0.0",
  "description": "vue-mushroom-admin docs",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./config": "./src/vitepress/config/baseConfig.js"
  },
  "files": [
    "src",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TothingWay/vue-mushroom-admin-docs.git"
  },
  "keywords": [
    "vue-mushroom-admin",
    "vue-mushroom-admin-docs",
    "vue-admin",
    "vite",
    "vue3"
  ],
  "author": "TothingWay",
  "license": "MIT",
  "homepage": "https://theme.vuejs.org",
  "bugs": {
    "url": "https://github.com/TothingWay/vue-mushroom-admin-docs/issues"
  },
  "scripts": {
    "lint": "prettier --check --write --parser typescript \"{__tests__,docs,src,types}/**/*.ts\"",
    "lint:fail": "prettier --check --parser typescript \"{__tests__,docs,src,types}/**/*.ts\"",
    "type": "tsc --noEmit",
    "test": "npm run lint && npm run type",
    "dev": "vitepress dev docs",
    "build": "vitepress build docs",
    "serve": "npm run build && vitepress serve docs",
    "prepare": "husky install",
    "cz": "czg"
  },
  "dependencies": {
    "@docsearch/css": "^3.2.1",
    "@docsearch/js": "^3.2.1",
    "@element-plus/icons-vue": "^2.0.9",
    "@vueuse/core": "^9.1.1",
    "body-scroll-lock": "^3.1.5",
    "element-plus": "^2.2.16",
    "normalize.css": "^8.0.1",
    "prism-theme-vars": "^0.2.4"
  },
  "devDependencies": {
    "@mdit-vue/types": "^0.11.0",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/escape-html": "^1.0.2",
    "@types/estree": "^1.0.0",
    "@types/markdown-it": "^12.2.3",
    "@types/markdown-it-container": "^2.0.5",
    "@types/node": "^18.7.14",
    "@types/prismjs": "^1.26.0",
    "chalk": "^4.1.2",
    "consola": "^2.15.3",
    "cz-git": "^1.3.10",
    "czg": "^1.3.10",
    "escape-html": "^1.0.3",
    "husky": "^8.0.0",
    "markdown-it": "^13.0.1",
    "markdown-it-container": "^3.0.0",
    "prettier": "^2.7.1",
    "prismjs": "^1.29.0",
    "sass": "^1.54.8",
    "typescript": "^4.8.2",
    "unocss": "^0.45.15",
    "vitepress": "^1.0.0-alpha.13",
    "vue": "^3.2.37"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
